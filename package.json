{
  "name": "ai-index",
  "version": "1.0.0", 
  "description": "AI-powered code indexing and search system for any codebase using OpenSearch and AWS Bedrock",
  "type": "module",
  "main": "lib/index.js",
  "bin": {
    "ai-index": "bin/ai-index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Homevote/ai-index.git"
  },
  "homepage": "https://github.com/Homevote/ai-index#readme",
  "bugs": {
    "url": "https://github.com/Homevote/ai-index/issues"
  },
  "author": "Homevote <dev@homevote.at>",
  "license": "MIT",
  "scripts": {
    "ai:index": "node tools/ai_index/build_index.js",
